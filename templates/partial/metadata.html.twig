
<table class="table table-condensed">
    <tbody>
    <tr>
        <th>Name</th>
        <td>{{ entity.name }}</td>
    </tr>
    <tr>
        <th>Label</th>
        <td>{{ entity.label }}</td>
    </tr>
    <tr>
        <th>Description</th>
        <td>{{ entity.description }}</td>
    </tr>
    <tr>
        <th>Created</th>
        <td>{{ entity.created|date }}</td>
    </tr>
    <tr>
        <th>Updated</th>
        <td>{{ entity.updated|date }}</td>
    </tr>
    </tbody>
</table>